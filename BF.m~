function img = BF(I,sizef,s1,s2)
sigmaG = s1;
sugmaR = s2;
f_coeff = Gaussain_filter(sigmaG,sizef);
offset = (sizef + 1)/2;
[r,c] = size(I);
new_I = zeros(r+2*(offset-1),c+2*(offset-1));
new_I(offset:r+offset-1,offset:c+offset-1) = I;
img = zeros(r,c);
for i = 2 : r+1
    for j = 2 : c+1
        lf = zeros(5,5);
        cen = new_I(i,j);
        for x = 1 : sizef
            for y = 1 : sizef
                lf(x,y) = exp(-(cen - new_I(i+x-offset,j+y-offset))^2/(2*sigmaR*sigmaR))*f_coeff(x,y);
            end
        end
        all_sum = 0;
        for k = 1 : sizef
            for l = 1 : sizef
                all
    end